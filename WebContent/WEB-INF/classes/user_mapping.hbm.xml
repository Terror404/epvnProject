<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.beans.profile.User" table="USER">
        <id name="idUser" type="java.lang.Integer" column="ID" length="10" />
        
        <property name="role" type="java.lang.Integer">
            <column name="ROLE" length="1" />
        </property>
                
        <property name="firstName" type="string">
            <column name="FIRSTNAME" length="255" />
        </property>
                
        <property name="lastName" type="string">
            <column name="LASTNAME" length="255" />
        </property>
        
        <property name="mailAddress" type="string">
            <column name="MAIL" length="255" />
        </property>
                
        <property name="password" type="string">
            <column name="PASSWORD" length="255" />
        </property>
        
        <property name="phoneNumber" type="java.lang.Integer">
            <column name="PHONE" length="10" />
        </property>
        
        <property name="dateAdherent" type="java.util.Date">
            <column name="DATE_ADHERENT" />
        </property>
        
        <property name="adminLevel" type="java.lang.Integer">
            <column name="ADMINLVL" length="1" />
        </property>
        
        <property name="physicalAddress" type="string">
            <column name="ADDRESS" length="255" />
        </property>

        <property name="zipCode" type="string">
            <column name="ZIPCODE" length="255" />
        </property>

        <property name="city" type="string">
            <column name="CITY" length="255" />
        </property>
        
        <property name="companyName" type="string">
            <column name="COMPANY_NAME" length="255" />
        </property>
        
        <property name="sirenNum" type="string">
            <column name="SIREN" length="255" />
        </property>


    </class>
</hibernate-mapping>