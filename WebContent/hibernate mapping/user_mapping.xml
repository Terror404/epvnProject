<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="user" table="USER">
        <id name="id" type="java.lang.Integer" column="ID" length="11" />
        
        <property name="firstName" type="string">
            <column name="FIRSTNAME" length="50" />
        </property>
        
        <property name="lastName" type="string">
            <column name="LASTNAME" length="50" />
        </property>
        
        <property name="mailAddress" type="string">
            <column name="MAIL_ADRESS" length="50" />
        </property>
        
        <property name="phone" type="string">
            <column name="PHONE" length="11" />
        </property>
        
        <property name="creationDate" type="date">
            <column name="CREATION_DATE" length="1" />
        </property>
        
        <property name="isAdherent" type="java.lang.Integer">
            <column name="IS_ADHERENT" precision="11" />
        </property>
        
        <property name="adminLevel" type="java.lang.Integer">
            <column name="ADMIN_LEVEL" precision="11" />
        </property>
        
        <property name="codeAdherent" type="java.lang.Integer">
            <column name="CODE_ADHERENT" precision="11" />
        </property>
        
        <property name="address" type="java.lang.Integer">
            <column name="ADDRESS" precision="11" />
        </property>
        
        <property name="zipCode" type="java.lang.Integer">
            <column name="ZIP_CODE" precision="5" />
        </property>
        
        <property name="city" type="string">
            <column name="CITY" precision="50" />
        </property>
        
        <property name="country" type="string">
            <column name="COUNTRY" precision="50" />
        </property>
        
        <property name="idCompanyInfos" type="java.lang.Integer">
            <column name="ID_COMPANY_INFOS" precision="11" />
        </property>
    </class>
</hibernate-mapping>