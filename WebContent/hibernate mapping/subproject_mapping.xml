<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="subproject" table="SUBPROJECT">
        <id name="id" type="java.lang.Integer" column="ID" length="11" />
        
        <property name="title" type="string">
            <column name="TITLE" length="50" />
        </property>
        
        <property name="description" type="string">
            <column name="DESCRIPTION" length="255" />
        </property>
        
        <property name="goal" type="java.lang.Integer">
            <column name="GOAL" length="11" />
        </property>
        
        <property name="actualAchievedGoal" type="java.lang.Integer">
            <column name="ACTUAL_ACHIEVED_GOAL" length="11" />
        </property>
        
        <property name="idProject" type="java.lang.Integer">
            <column name="ID_PROJECT" length="11" />
        </property>
        
        <property name="numberSubproject" type="java.lang.Integer">
            <column name="NUMBER_SUBPROJECT" length="50" />
        </property>
        
        <property name="author" type="string">
            <column name="AUTHOR" length="50" />
        </property>
    </class>
</hibernate-mapping>