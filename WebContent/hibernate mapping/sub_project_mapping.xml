<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="subProject" table="SUB_PROJECT">
        <id name="id" type="java.lang.Integer" column="ID" length="10" />
        
        <property name="title" type="string">
            <column name="TITLE" length="255" />
        </property>
        
        <property name="description" type="string">
            <column name="DESCRIPTION" length="4000" />
        </property>
        
        <property name="donated" type="java.lang.Decimal">
            <column name="DONATED" length="15" precision="2" />
        </property>
        
        <property name="goal" type="java.lang.Decimal">
            <column name="GOAL" length="15" precision="2" />
        </property>
        
        <property name="projectID" type="java.lang.Integer">
            <column name="PROJECT_ID" length="10" />
        </property>

    </class>
</hibernate-mapping>