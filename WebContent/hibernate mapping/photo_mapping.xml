<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="photo" table="PHOTO">
        <id name="id" type="java.lang.Integer" column="ID" length="11" />
        
        <property name="idProject" type="java.lang.Integer">
            <column name="ID_PROJECT" length="11" />
        </property>
        
        <property name="idSubroject" type="java.lang.Integer">
            <column name="ID_SUBPROJECT" length="11" />
        </property>
        
        <property name="photoNumber" type="java.lang.Integer">
            <column name="PHOTO_NUMBER" length="11" />
        </property>
        
        <property name="title" type="string">
            <column name="TITLE" length="50" />
        </property>
        
        <property name="photoPath" type="string">
            <column name="PHOTO_PATH" length="50" />
        </property>
    </class>
</hibernate-mapping>